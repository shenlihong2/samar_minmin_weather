<!--index.wxml-->
<view class="container" >
    <view class="title-name">敏敏小天气</view>
     <view class="update-time">最后更新于{{update_time}}</view>
     <swiper current="{{currentTab}}" class="swiper-box" duration="300" style="height:{{winHeight - 31}}px" bindchange="bindChange">
     <swiper-item>
      <view class="weather-card">
            <view class="weather-top">
               <view class="weather-top-top">
                    <view class="range">{{tem2}}°C~ {{tem1}}°C</view>
                    <view class="refresh" >
                    <image src="../../images/refesh.png" animation='{{animation}}' bindtap="g"></image>
                    </view>
               </view>
               <view class="weather-top-center">
               <image src="../../images/{{wea_img}}.png" ></image>
               </view>
               <view class="weather-information">{{wea}}</view>
               <view class="weather-top-bottom">
                   <view class="weather-temperature">{{tem}}°C</view>
                   <view class="other-information">
                       <view class="location">
                           <view class="location-image">
                           <image  src="../../images/location.png"></image>
                           </view>
                           <view class="location-information">{{city}}</view>
                       </view>
                       <view class="weekday">{{week}}</view>
                   </view>
               </view>
            </view>
            <view class="weather-bottom">
            <view class="weather-left">
                 <view class='tomorrow'>明天</view>
                 <view class='weather1'>
                 <image src="../../images/gray/{{week1wea_img}}2.png" ></image>
                 </view>
                 <view class='weather1-information'>{{week1wea}}</view>
                 <view class='weather1-range'>{{week1tem2}}~ {{week1tem1}}</view>
            </view>
            <view class="weather-center">
                 <view class='the-day-after-tomorrow'>{{week2}}</view>
                 <view class='weather2'>
                 <image src="../../images/gray/{{week2wea_img}}2.png" ></image>
                 </view>
                 <view class='weather2-information'>{{week2wea}}</view>
                 <view class='weather2-range'>{{week2tem2}}~ {{week2tem1}}</view>
            </view>
            <view class="weather-right">
                 <view class='the-day-after-tomorrow-tomorrow'>{{week3}}</view>
                 <view class='weather3'>
                 <image src="../../images/gray/{{week3wea_img}}2.png" ></image>
                 </view>
                 <view class='weather3-information'>{{week3wea}}</view>
                  <view class='weather3-range'>{{week3tem2}}~ {{week3tem1}}</view>
            </view>
            </view>
      </view> 
    </swiper-item>
    
     <swiper-item>
     <view class="laba">
       <image src="../../images/laba.png" ></image>
     </view>
      <view  class='tongzhitext'>
      <text class="tongzhi-text">{{text}}</text>
  </view>
  <view class="second-top">
     <view class="Left">
       <view id="top-left-image" class="some-image">
       <image src="data:image/png;base64," ></image>
       </view>
       <view id="top-left-information" class="some-information">空气质量：{{air_level}}</view>
     </view>
    <view class="Center">
        <view id="top-center-image" class="some-image">
        <image src="data:image/png;base64," ></image>
        </view>
        <view id="top-center-information" class="some-information">质量指数：{{air}}</view>
     </view>
      <view class="Right">
        <view id="top-right-image" class="some-image">
         <image src="data:image/png;base64," ></image>
        </view>
        <view id="top-right-information" class="some-information" >PM2.5：{{air_pm25}}</view>
     </view>
  </view>
 <view class="second-center">
   <view class="Left">
       <view id="center-left-image" class="some-image">
        <image src="data:image/png;base64," ></image>
       </view>
       <view id="center-left-information" class="some-information">{{tem2}}°C~ {{tem1}}°C</view>
     </view>
    <view class="Center">
        <view id="center-center-image" class="some-image">
         <image src="data:image/png;base64," ></image>
        </view>
        <view id="center-center-information"  class="some-information">湿度：{{humidity}}</view>
     </view>
      <view class="Right">
        <view id="center-right-image" class="some-image">
         <image src="data:image/png;base64," ></image>
        </view>
        <view id="center-right-information"  class="some-information">气压：{{pressure}}hPa</view>
     </view>
  </view>
   <view class="second-bottom">
     <view class="Left">
       <view id="bottom-left-image" class="some-image">
        <image src="data:image/png;base64," ></image>
       </view>
       <view id="bottom-left-information"  class="some-information">风向：{{win}}</view>
     </view>
    <view class="Center">
        <view id="bottom-center-image" class="some-image">
         <image src="data:image/png;base64," ></image>
        </view>
        <view id="bottom-center-information"  class="some-information">风速：{{win_meter}}</view>
     </view>
      <view class="Right">
        <view id="bottom-right-image" class="some-image">
         <image src="data:image/png;base64," ></image>
        </view>
        <view id="bottom-right-information"  class="some-information">能见度：{{visibility}}</view>
     </view>
  </view>
    </swiper-item>
     </swiper>
      <view class="swiper-tab">  
    <view class="swiper-tab-list {{currentTab==0 ? 'on' : 'off'}}" data-current="0" bindtap="swichNav"></view>  
   <view class="swiper-tab-list {{currentTab==1 ? 'on' : 'off'}}" data-current="1" bindtap="swichNav"></view>    
</view> 
</view>
